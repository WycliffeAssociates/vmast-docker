{
  "name": "nova-framework/framework",
  "type": "project",
  "description": "Framework for PHP 8",
  "homepage": "https://github.com/nova-framework/framework",
  "license": "MIT",
  "authors": [
    {
      "name": "David Carr",
      "email": "dave@daveismyname.com",
      "homepage": "http://daveismyname.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "~8.3",
    "symfony/console": "~7.1",
    "symfony/finder": "~7.1",
    "symfony/http-foundation": "~7.1",
    "symfony/http-kernel": "~7.1",
    "symfony/routing": "~7.1",
    "symfony/yaml": "~7.1",
    "symfony/mime": "~7.1",
    "doctrine/inflector": "~2.0",
    "phpmailer/phpmailer": "~6.9",
    "patchwork/utf8": "~1.3",
    "nesbot/carbon": "~3.7.0",
    "swiftmailer/swiftmailer": "~6.2",
    "twbs/bootstrap": "~5.3",
    "monolog/monolog": "~3.7",
    "predis/predis": "~2.2",
    "tecnickcom/tcpdf": "~6.7",
    "guzzlehttp/guzzle": "~7.9",
    "sentry/sdk": "~4.0.0",
    "filp/whoops": "~2.15",
    "myclabs/php-enum": "~1.8",
    "vlucas/phpdotenv": "~5.6",
    "php-amqplib/php-amqplib": ">=3.0",
    "usfmphp/usfmparser":  "~0.1",
    "ext-curl": "*",
    "ext-sockets": "*",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Shared\\": "shared/",
      "": "system/"
    },
    "files": [
      "system/functions.php"
    ]
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require-dev": {}
}
